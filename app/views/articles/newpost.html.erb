<h1>New Article</h1>

<p>Please insert a new article</p>

<%= form_for :article, url: articles_path do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.label :text %><br>
    <%= f.text_area :text %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>

<%= link_to 'new_article_path', root_path %>
<br>
<%= link_to 'articles_path', articles_path %>
